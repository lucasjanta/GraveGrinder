config_version=5

[application]

config/name="MomentumJam"
run/main_scene="res://Scenes/Menus/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

UI="*res://Scripts/on_screen_ui.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
CameraHandle="*res://Scripts/camera_handle.gd"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1152
window/size/window_height_override=648
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

[global_group]

stopwatch="speedrun timer group"

